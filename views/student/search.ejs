<div
  class="d-flex justify-content-between flex-wrap flex-md nowrap align-items-center pt-3 pb-2 mb-3 border-bottom"
>
  <h1 class="h2">Find Results</h1>
</div>
<%- include('../partials/_flashMsg') %>
<!--/Search Form Handling-->
<form action="/searchKey" id="search_student">
  <div class="new_user">
    <div class="form-group">
      <label for="rollNo" class="" text-light>Roll No</label>
      <input type="hidden" name="id" value="" />
      <input type="text" name="rollNo" value="" placeholder="123.." required />
    </div>
    <div class="form-group">
      <label for="dob" class="" text-light>Date of Birth</label>
      <input
        type="date"
        id="date"
        name="dob"
        value=""
        placeholder="12/01/2000"
        required
      />
    </div>

    <div class="form-group">
      <button type="submit" class="btn btn-primary">Search</button>
    </div>
  </div>
</form>
<!--/Search Form Handling-->
